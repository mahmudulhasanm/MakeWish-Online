<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#ddd"
    tools:context=".Android">

<ScrollView
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:ignore="UselessParent">

        <LinearLayout
            android:layout_width="match_parent"
            android:orientation="vertical"
            android:layout_height="wrap_content">
            <LinearLayout
                android:id="@+id/banner_container"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"/>

            <LinearLayout
                android:layout_width="match_parent"
                android:background="@drawable/content"
                android:orientation="vertical"
                android:layout_margin="10sp"
                android:padding="10sp"
                android:id="@+id/collection"
                android:layout_height="wrap_content">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="#000"
                    android:layout_gravity="center"
                    android:textSize="16sp"
                    android:fontFamily="@font/neo_latina"
                    android:layout_marginTop="10sp"
                    android:textStyle="bold"
                    android:text="@string/money_making_by_android_apps"/>
                <ImageView
                    android:layout_width="match_parent"
                    android:layout_height="200sp"
                    android:layout_margin="15sp"
                    android:layout_gravity="center"
                    android:src="@drawable/android" />
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textStyle="normal"
                    android:textSize="15sp"
                    android:layout_marginBottom="10sp"
                    android:fontFamily="@font/opensense"
                    android:textColor="#000"
                    android:text="@string/android_content"/>
                <FrameLayout
                    android:id="@+id/youtube_fragment"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_centerHorizontal="true"
                    android:visibility="visible" />
                <TextView
                    android:layout_width="wrap_content"
                    android:background="#4CAF50"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10sp"
                    android:padding="5sp"
                    android:fontFamily="@font/opensense"
                    android:textStyle="bold"
                    android:textSize="15sp"
                    android:textColor="#fff"
                    android:text="@string/content"/>
            </LinearLayout>
            <LinearLayout
                android:layout_width="wrap_content"
                android:background="@drawable/circle2"
                android:orientation="horizontal"
                android:layout_gravity="center"
                android:id="@+id/android_app"
                android:layout_marginRight="20sp"
                android:layout_marginLeft="20sp"
                android:layout_height="wrap_content">
                <ImageView
                    android:layout_width="100sp"
                    android:layout_height="70sp"
                    android:padding="5sp"
                    android:layout_gravity="center"
                    android:layout_marginLeft="40sp"
                    android:src="@drawable/android" />
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:textStyle="bold"
                    android:gravity="center"
                    android:textSize="15sp"
                    android:layout_marginRight="20sp"
                    android:fontFamily="@font/opensense"
                    android:textColor="@color/colorPrimary"
                    android:text="@string/see_our_all_apps_amp_pricing"/>
            </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:background="@drawable/content"
        android:orientation="vertical"
        android:layout_margin="10sp"
        android:padding="10sp"
        android:layout_height="wrap_content">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:padding="5sp"
            android:textStyle="bold"
            android:textSize="19sp"
            android:textColor="#000"
            android:text="@string/our_services"/>
        <TextView
            android:layout_width="wrap_content"
            android:background="#4CAF50"
            android:layout_height="wrap_content"
            android:padding="5sp"
            android:textStyle="bold"
            android:fontFamily="@font/opensense"
            android:textSize="15sp"
            android:textColor="#fff"
            android:text="@string/content1"/>
        </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:background="@drawable/content"
                android:orientation="vertical"
                android:layout_margin="10sp"
                android:padding="10sp"
                android:layout_height="wrap_content">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_marginTop="10sp"
                    android:text="@string/which_things_did_you_need_to_know_for_work_with_android_apps"
                    android:textColor="#000"
                    android:fontFamily="@font/opensense"
                    android:textSize="16sp"
                    android:textStyle="bold" />
                <LinearLayout
                    android:layout_width="match_parent"
                    android:orientation="horizontal"
                    android:layout_marginTop="10sp"
                    android:layout_height="50sp">
                    <ImageView
                        android:layout_width="50sp"
                        android:src="@drawable/studio"
                        android:layout_height="wrap_content" />
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="10sp"
                        android:textSize="16sp"
                        android:textColor="#000"
                        android:fontFamily="@font/opensense"
                        android:textStyle="bold"
                        android:layout_gravity="center"
                        android:text="@string/android_studio1"/>
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:orientation="horizontal"
                    android:layout_marginTop="10sp"
                    android:layout_height="50sp">
                    <ImageView
                        android:layout_width="50sp"
                        android:src="@drawable/consol"
                        android:layout_height="wrap_content" />
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="10sp"
                        android:textSize="16sp"
                        android:fontFamily="@font/opensense"
                        android:textStyle="bold"
                        android:textColor="#000"
                        android:layout_gravity="center"
                        android:text="@string/google_play_console"/>
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:orientation="horizontal"
                    android:layout_marginTop="10sp"
                    android:layout_height="50sp">
                    <ImageView
                        android:layout_width="50sp"
                        android:src="@drawable/admob"
                        android:layout_height="wrap_content" />
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="10sp"
                        android:textSize="16sp"
                        android:fontFamily="@font/opensense"
                        android:textStyle="bold"
                        android:textColor="#000"
                        android:layout_gravity="center"
                        android:text="@string/admob1"/>
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:orientation="horizontal"
                    android:layout_marginTop="10sp"
                    android:layout_height="50sp">
                    <ImageView
                        android:layout_width="50sp"
                        android:src="@drawable/adsense"
                        android:layout_height="wrap_content" />
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="10sp"
                        android:fontFamily="@font/opensense"
                        android:textStyle="bold"
                        android:textSize="16sp"
                        android:textColor="#000"
                        android:layout_gravity="center"
                        android:text="@string/adsense1"/>
                </LinearLayout>
                <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textStyle="bold"
                android:fontFamily="@font/opensense"
                android:textSize="16sp"
                android:layout_marginTop="20sp"
                android:textColor="#000"
                android:text="@string/android_studio"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textStyle="normal"
                    android:textSize="15sp"
                    android:fontFamily="@font/opensense"
                    android:textColor="#000"
                    android:text="@string/android_studio_content"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textStyle="bold"
                    android:textSize="16sp"
                    android:fontFamily="@font/opensense"
                    android:layout_marginTop="20sp"
                    android:textColor="#000"
                    android:text="@string/what_is_play_console"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textStyle="normal"
                    android:textSize="15sp"
                    android:fontFamily="@font/opensense"
                    android:textColor="#000"
                    android:text="@string/play_console"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textStyle="bold"
                    android:textSize="16sp"
                    android:fontFamily="@font/opensense"
                    android:layout_marginTop="20sp"
                    android:textColor="#000"
                    android:text="@string/what_is_admob"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textStyle="normal"
                    android:textSize="15sp"
                    android:textColor="#000"
                    android:fontFamily="@font/opensense"
                    android:text="@string/admob"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textStyle="bold"
                    android:textSize="16sp"
                    android:fontFamily="@font/opensense"
                    android:layout_marginTop="20sp"
                    android:textColor="#000"
                    android:text="@string/what_is_adsense"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textStyle="normal"
                    android:textSize="15sp"
                    android:fontFamily="@font/opensense"
                    android:textColor="#000"
                    android:text="@string/adsense"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:background="#4CAF50"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10sp"
                    android:padding="5sp"
                    android:textStyle="bold"
                    android:fontFamily="@font/opensense"
                    android:textSize="16sp"
                    android:textColor="#fff"
                    android:text="@string/contact_with"/>
            </LinearLayout>
        </LinearLayout>

</ScrollView>
</LinearLayout>